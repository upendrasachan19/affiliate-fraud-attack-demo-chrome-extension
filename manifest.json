{
  "name": "Khao-Cookie",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Affiliate fraud attack",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "browser action demo",
    "default_popup": "index.html"
  },
  "permissions": [
    "cookies",
    "tabs",
     "http://*/",
    "https://*/"
  ]
}